{
  "name": "podcastsuite",
  "version": "1.0.16",
  "description": "A set of utilities to work with Podcasts",
  "source": "index.ts",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "umd:main": "dist/index.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/navio/podcastsuite.git"
  },
  "scripts": {
    "start": "microbundle-crl index.ts",
    "test": "jest",
    "prepublish": "npm start && npm test"
  },
  "keywords": [
    "Podcast"
  ],
  "author": "Alberto Navarro",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "idb-keyval": "^3.2.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "fake-indexeddb": "^2.1.1",
    "fetch-mock": "^7.7.3",
    "jest": "^24.9.0",
    "microbundle-crl": "^0.13.9",
    "node-fetch": "^2.6.0"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "xml",
      "node"
    ]
  }
}
